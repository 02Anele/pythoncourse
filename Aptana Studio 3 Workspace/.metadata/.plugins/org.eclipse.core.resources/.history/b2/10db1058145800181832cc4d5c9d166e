def greeter(name, surname=''):
    'greeter function'
    return 'hello {} {}'.format(name, surname)
    
def square(x, y=2):
    return x ** y

def tree(n=5):
    out = ''
    for i in range(1,n+1):
        out += ('* '*i).center(n*2) + '\n'
    return out

print(tree(10))